use("github.com/softkitteh/repokitteh-modules/kitteh.sky")
use("github.com/softkitteh/repokitteh-modules/doge.sky")
use("github.com/softkitteh/repokitteh-modules/assign.sky")

def on_command(command):
  if command.name == 'x':
    ls = github_issue_labels()
    github_issue_create_comment('%s' % ls)
    if 'x' in ls:
      github_issue_label('y')
      github_issue_unlabel('x')
    else:
      github_issue_label('x')
      github_issue_unlabel('y')
